<table class="view-table">
	<tr class="view-headers"> <!-- Headers -->
		<td class="whitespace-nowrap">Time Slot</td>
		<td>Integrity <br>(10 Pax)</td>
		<td>Innovation (12 Pax)</td>
		<td>Teamwork (16 Pax)</td>			
	</tr>					

	<tbody id="schedRows" class="view-body"><!-- Time Table -->
		{{#each meetingRows}} <tr>
			<td>{{time}}</td>
			{{#unless integTaken}}
				<td rowspan={{integSlots}} class="{{#if integSlotTaken}}takenSlot{{/if}} {{#if integOwn}}own{{/if}} {{#if integID includeZero=true}}Integrity_{{integID}}{{/if}}"></td>
			{{/unless}}
			{{#unless innovTaken}}
				<td rowspan={{innovSlots}} class="{{#if innovSlotTaken}}takenSlot{{/if}} {{#if innovOwn}}own{{/if}} {{#if innovID includeZero=true}}Innovation_{{innovID}}{{/if}}"></td>
			{{/unless}}
			{{#unless teamTaken}}
				<td rowspan={{teamSlots}} class="{{#if teamSlotTaken}}takenSlot{{/if}} {{#if teamOwn}}own{{/if}} {{#if teamID includeZero=true}}Teamwork_{{teamID}}{{/if}}"></td>
			{{/unless}}
		</tr> {{/each}}
	</tbody>
</table>